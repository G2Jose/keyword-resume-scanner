bs_package_flags = -bs-package-map reason
src_root_dir = /Users/georgejose/dev_files/stupid-resume-scanner
bsc = /Users/georgejose/dev_files/stupid-resume-scanner/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/georgejose/dev_files/stupid-resume-scanner/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/Users/georgejose/dev_files/stupid-resume-scanner/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = -ns Reason
bsb_dir_group = 0
refmt = /Users/georgejose/dev_files/stupid-resume-scanner/node_modules/bs-platform/lib/refmt3.exe
reason_react_jsx = 
refmt_flags = --print binary
bsc_lib_includes = -I . -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/utils.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/utils.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/utils.mlast.d : build_deps src/utils.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/utils-Reason.cmj |  $src_root_dir/src/utils.bs.js src/utils-Reason.cmi : build_cmj_cmi src/utils.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/CommandLineArgs.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/CommandLineArgs.re
build src/CommandLineArgs.mlast.d : build_deps src/CommandLineArgs.mlast
build src/CommandLineArgs-Reason.cmj |  $src_root_dir/src/CommandLineArgs.bs.js src/CommandLineArgs-Reason.cmi : build_cmj_cmi src/CommandLineArgs.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build Reason.cmi : build_package Reason.mlmap
build build.ninja : phony  ||  Reason.cmi src/CommandLineArgs.mlast.d src/utils.mlast.d
